{
    "version":2,
    "builds":[
        {
            "src":"main.ts",
            "use": "@vercel/node"
        }
    ],
    "routes":[
        {
            "src":"/(.*)",
            "dest":"/api/main.ts",
            "methods":[
                "GET",
                "POST",
                "PUT",
                "PATCH",
                "DELETE"
            ]
        }
    ]
}